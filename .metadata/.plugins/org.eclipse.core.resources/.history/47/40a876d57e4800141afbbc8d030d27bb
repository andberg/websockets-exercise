package se.berglund.messagehandlers;

import se.berglund.models.Message;

public class MessageHandler {

	private Message message; 
	private static MessageHandler instance = null; 
	
	private MessageHandler(Message message){
		this.message = message; 
	}
	
	public static MessageHandler getInstance(message){
		if(MessageHandler.instance == null){
			MessageHandler.instance = new MessageHandler(message); 
		}
		return MessageHandler.instance; 
	}
}
